<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControllerStateTransition">
   <children>
      <VBox prefHeight="700.0" prefWidth="800.0" spacing="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="60.0" prefWidth="800.0">
               <children>
                  <Button graphicTextGap="5.0" layoutX="14.0" layoutY="14.0" lineSpacing="4.0" mnemonicParsing="false" onAction="#gotoHome" prefHeight="60.0" prefWidth="160.0" text="Home" textFill="#726d6d">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </HBox.margin>
                  </Button>
                  <Button graphicTextGap="5.0" lineSpacing="4.0" mnemonicParsing="false" onAction="#gotoStartingState" prefHeight="60.0" prefWidth="160.0" text="Starting State" textFill="#726d6d">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </HBox.margin>
                  </Button>
                  <Button graphicTextGap="5.0" layoutX="4.0" lineSpacing="4.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="160.0" text="State Transition" textFill="#d33636">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </HBox.margin>
                  </Button>
                  <Button graphicTextGap="5.0" layoutX="4.0" lineSpacing="4.0" mnemonicParsing="false" onAction="#gotoStateEmission" prefHeight="60.0" prefWidth="160.0" text="Emission" textFill="#726d6d">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="350.0" prefWidth="200.0">
               <children>
                  <ScrollPane fx:id="scrolpane" prefHeight="350.0" prefWidth="400.0">
                     <content>
                        <VBox fx:id="vbox" prefHeight="350.0" prefWidth="400.0" />
                     </content>
                  </ScrollPane>
                  <StackPane prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#buttonProcess" text="process" textFill="#cd2e2e">
                           <font>
                              <Font name="System Bold" size="18.0" />
                           </font>
                        </Button>
                     </children>
                  </StackPane>
                  <PieChart legendSide="TOP" prefHeight="200.0" prefWidth="300.0" title="state transition" titleSide="BOTTOM" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
