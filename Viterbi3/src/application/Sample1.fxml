<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="393.0" prefWidth="308.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <Label fx:id="header" layoutX="23.0" layoutY="14.0" prefHeight="25.0" prefWidth="221.0" text="Welcome to Viterbi GUI" textFill="#9a0b0b">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <VBox layoutY="41.0" prefHeight="359.0" prefWidth="300.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="41.0">
         <children>
            <HBox prefHeight="53.0" prefWidth="308.0" spacing="10.0">
               <children>
                  <TextField fx:id="stateName" prefHeight="20.0" prefWidth="200.0" promptText="Enter State name">
                     <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </HBox.margin>
                  </TextField>
                  <Button mnemonicParsing="false" onAction="#AddState" text="ADD">
                     <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </HBox.margin>
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <StackPane prefHeight="38.0" prefWidth="308.0">
               <children>
                  <Label fx:id="stateCounter" prefHeight="18.0" prefWidth="92.0" />
               </children>
            </StackPane>
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="53.0" prefWidth="308.0" spacing="10.0">
               <children>
                  <TextField fx:id="emojiName" prefHeight="20.0" prefWidth="200.0" promptText="Enter Emoji name">
                     <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </HBox.margin>
                  </TextField>
                  <Button mnemonicParsing="false" onAction="#addEmo" text="ADD">
                     <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </HBox.margin>
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <StackPane layoutX="10.0" layoutY="87.0" prefHeight="35.0" prefWidth="308.0">
               <children>
                  <Label fx:id="emoCounter" prefHeight="17.0" prefWidth="92.0" />
               </children>
            </StackPane>
            <TableView fx:id="tableview" prefHeight="155.0" prefWidth="308.0">
              <columns>
                <TableColumn fx:id="state1" onEditCommit="#ChangedCellss" prefWidth="121.0" text="Starting State" />
                <TableColumn fx:id="probability" onEditCommit="#ChangedCellsp" prefWidth="144.0" text="Starting Probability" />
              </columns>
            </TableView>
            <HBox prefHeight="64.0" prefWidth="308.0">
               <children>
                  <TextField fx:id="ss" promptText="Starting State" />
                  <TextField fx:id="sp" layoutX="10.0" layoutY="10.0" promptText="Starting probability" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" />
            <StackPane layoutX="10.0" layoutY="218.0" prefHeight="54.0" prefWidth="300.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#newRowPushed1" text="Add a Row">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
               </children>
            </StackPane>
            <StackPane prefHeight="54.0" prefWidth="300.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#gotoTableview" text="NEXT">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
